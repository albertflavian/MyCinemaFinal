@{
    if (Session["user"] == null)
    {
        <script type="text/javascript">window.location.href = "/User/Login";</script>
        //Response.Redirect ("/User/Login",true);
    }
    else
        if (Session["user"].ToString() != "admin")    //see login
    {
        <script type="text/javascript">history.go(-1);</script>
    }

}


<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">
    <meta content="ie=edge" http-equiv="X-UA-Compatible">

    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css">
    <link href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,500,600,700,700i" rel="stylesheet">
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" type="text/css" rel="stylesheet"
          media="screen" />
    <link media="screen" rel="stylesheet" type="text/css" href="../admin-menu-ip/admin-menu-ip.css" />
    <link media="screen" rel="stylesheet" type="text/css" href="../../StyleSheets/Menu.css" />
    <link rel="stylesheet" type="text/css" href="/Content/lightbox.css">
    <link rel="stylesheet" type="text/css" href="/Content/lightbox.min.css">
    <link rel="stylesheet" href="https://www.w3schools.com/w3css/4/w3.css">
    <link media="screen" rel="stylesheet" type="text/css" href="../Content/images.css">
    <link media="screen" rel="stylesheet" type="text/css" href="/Content/admin-menu-ip/admin-menu-ip.css" />
    <link media="screen" rel="stylesheet" type="text/css" href="/Content/admin-home-ip.css" />
    <link href="/Content/timingfield/timingfield.css" type="text/css" rel="stylesheet" media="screen" />
    <link media="screen" rel="stylesheet" type="text/css" href="/Content/admin-menu-ip.css">
    <link media="screen" rel="stylesheet" type="text/css" href="/Content/admin-movies-ip.css">
    <link media="screen" rel="stylesheet" type="text/css" href="../../StyleSheets/Dropdown.css" />
    <link href="//netdna.bootstrapcdn.com/bootstrap/3.1.0/css/bootstrap.min.css" rel="stylesheet" id="bootstrap-css">
    <script src="//netdna.bootstrapcdn.com/bootstrap/3.1.0/js/bootstrap.min.js"></script>
    <script src="//code.jquery.com/jquery-1.11.1.min.js"></script>
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div class="layout">
        <div class="l-left">
            <a class="toggle-menu topm1" href="#"><i class="material-icons">menu</i></a>
            <a class="logo-menu" href="javascript:window.location.href=window.location.href" style="height:55px;">Admin Panel</a>
            <div class="menu">
                <a href="@Url.Action("Admin", "Home")" class="menu-item" onclick="currentMenu(1)">
                    <img src="../../images/icon_home-1.svg" alt="" class="ico">
                    Home
                </a>

                <a id="2" href="@Url.Action("IndexAdmin","Movies")" class="menu-item" onclick="currentMenu(3)">
                    <img src="../../images/icon_products-1.svg" alt="" class="ico">
                    Movies
                </a>
                <a href="@Url.Action("IndexAdmin", "Rooms")" class="menu-item" onclick="currentMenu(4)">
                    <img src="../../images/icon_Invoices-1.svg" alt="" class="ico">
                    Cinema Rooms
                </a>
                <a href="@Url.Action("Index","Timetables")" class="menu-item" onclick="currentMenu(5)">
                    <img src="../../images/icon_calendar-1.svg" alt="" class="ico">
                    Program
                </a>
                <a href="@Url.Action("IndexAdmin", "User")" class="menu-item" onclick="currentMenu(6)">
                    <img src="../../images/icon_customers-1.svg" alt="" class="ico">
                    Users
                </a>
                <a href="@Url.Action("Index", "Home")" class="menu-item" onclick="currentMenu(1)">
                    <img src="../../images/icon_home-1.svg" alt="" class="ico">
                    Back To Home
                </a>
            </div>
        </div>
        <div class="l-right">
            <div class="top-bar" style="height:50px;">
                <form action="" class="left">
                    <a class="toggle-menu topm2" href="#"><i class="material-icons">menu</i></a>
                    <img alt="" class="icon" src="../images/icon_search.svg">
                    <input class="Search" id="input_search" placeholder="Search" type="text">
                </form>

                <div class="right" style="padding-right: 30px;">

                    <button type="button" class="btn" data-toggle="dropdown">
                        <img alt="" class="notif-ico" src="../../images/icon_notification.svg" />
                    </button>
                    <ul class="dropdown-menu scrollable-menu" role="menu">
                        <li><a href="#">Action</a></li>
                        <li><a href="#">Another action</a></li>
                        <li><a href="#">Something else here</a></li>
                        <li><a href="#">Action</a></li>
                        <li><a href="#">Another action</a></li>
                        <li><a href="#">Something else here</a></li>
                        <li><a href="#">Action</a></li>
                        <li><a href="#">Another action</a></li>
                        <li><a href="#">Something else here</a></li>
                        <li><a href="#">Action</a></li>
                        <li><a href="#">Another action</a></li>
                        <li><a href="#">Something else here</a></li>
                        <li><a href="#">Action</a></li>
                        <li><a href="#">Another action</a></li>
                    </ul>


                    <div class="dropdown">
                        <div>

                            <span>
                                @Session["FirstName"] @Session["LastName"]
                            </span>
                            <img alt="" class="arrow" src="../../images/icon_down-arrow-small.svg">
                        </div>

                        <div class="dropdown-content">
                            <br />
                            <div class="dropdown" style="padding-left: 10px;">

                                <span>
                                    <a href="@Url.Action("Settings", "Home")" style=" text-decoration:none;  color: black;">
                                        <img alt="" class="notif-ico" src="../images/Settings.png" />
                                        Settings
                                    </a>
                                </span>
                            </div>


                            @using (Html.BeginForm("Logout", "User", FormMethod.Post, new { }))
                            {
                                @Html.AntiForgeryToken()
                                <button type="submit" class="btn btn-link" style="   color: black; text-decoration: none; align-items:left;">
                                    <img alt="" class="notif-ico" src="../images/Logout.png" /> Logout
                                </button>

                            }
                        </div>
                    </div>





                </div>
            </div>


            @RenderBody()
            @Scripts.Render("~/bundles/jquery")
            @Scripts.Render("~/bundles/bootstrap")
            @RenderSection("scripts", required: false)
        </div>
    </div>

    <script src="https://code.jquery.com/jquery-3.3.1.slim.min.js"
            integrity="sha384-q8i/X+965DzO0rT7abK41JStQIAqVgRVzpbzo5smXKp4YfRvH+8abtTE1Pi6jizo"
            crossorigin="anonymous"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"
            integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1"
            crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"
            integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM"
            crossorigin="anonymous"></script>
    <script src="/Scripts/script.js"></script>
    <script src="/Scripts/home.js"></script>
    <script src="/Scripts/lightbox-plus-jquery.min.js"></script>
</body>
</html>